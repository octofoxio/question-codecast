      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/mongodb": "^3.1.19",
    "@types/react": "^16.7.18",
    "apollo-boost": "^0.1.27",
