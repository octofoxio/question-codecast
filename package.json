  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon --watch 'server.ts' --exec 'ts-node' server.ts"
  },
  "jest": {
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
